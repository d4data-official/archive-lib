{
  "name": "@d4data/archive-lib",
  "description": "Library to extract and normalize data from GDPR personal data archives",
  "license": "GPL-3.0",
  "version": "0.1.0-alpha.10",
  "repository": {
    "type": "git",
    "url": "https://github.com/d4data-official/archive-lib.git"
  },
  "main": "dist/src/index.js",
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "node .",
    "build": "cross-env NODE_ENV=production rimraf dist && ttsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev -C ttypescript --respawn --transpile-only -- src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "ava --timeout=5m -v",
    "test:cov": "nyc npm run test",
    "debug": "cross-env NODE_ENV=test ts-node --project debug/tsconfig.json -C ttypescript --transpile-only -- debug/debug.ts",
    "doc:generate": "typedoc src/index.ts --out docs/api/html",
    "prepare": "husky install",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "engines": {
    "node": ">=15"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/utils/**/*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "environmentVariables": {
      "TS_NODE_COMPILER": "ttypescript"
    }
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "jsdom": "^16.5.1",
    "mailparser": "^3.2.0",
    "mbox-parser": "^1.0.1",
    "moment": "^2.29.1",
    "papaparse": "^5.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "typescript-is": "^0.18.2",
    "uuid": "^8.3.2",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.7",
    "@types/mailparser": "^3.0.2",
    "@types/node": "^14.14.26",
    "@types/papaparse": "^5.2.5",
    "@types/uuid": "^8.3.0",
    "@types/yauzl": "^2.9.1",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "ava": "^3.15.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.0.9",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0",
    "ttypescript": "^1.5.12",
    "typedoc": "^0.21.5"
  },
  "keywords": [
    "d4data",
    "GDPR",
    "personal data",
    "archive",
    "facebook",
    "google",
    "discord",
    "reddit"
  ]
}
