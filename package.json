{
  "name": "d4data-archive-lib",
  "license": "MIT",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "husky": "husky //not configurate",
    "build": "cross-env NODE_ENV=production rimraf dist && tsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -- src/index.ts",
    "document": "typedoc ./src; cp -r doc ./docs/doc",
    "depcheck": "depcheck --ignores=\"eslint,babel-*,@classes/*,types/schemas/*,modules/*,@services,classes/standardizer/*,classes/archive/*\"",
    "lint": "eslint --fix .",
    "try": "ts-node",
    "test": "ava -v"
  },
  "engines": {
    "node": "^15"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "reflect-metadata": "^0.1.13",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.26",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "ava": "^3.14.0",
    "depcheck": "^1.4.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.0.9",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.0.0",
    "typedoc": "^0.20.28",
    "typedoc-plugin-markdown": "^3.5.0"
  }
}
