{
  "name": "d4data-archive-lib",
  "version": "0.1.0",
  "main": "dist/index.js",
  "scripts": {
    "start": "node .",
    "build": "cross-env NODE_ENV=production rimraf dist && ttsc",
    "dev": "cross-env NODE_ENV=development ts-node-dev -C ttypescript --respawn --transpile-only -- src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "ava --timeout=5m -v",
    "prepare": "husky install"
  },
  "engines": {
    "node": "^15"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/utils/**/*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "environmentVariables": {
      "TS_NODE_COMPILER": "ttypescript"
    }
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "iconv-lite": "^0.6.2",
    "jsdom": "^16.5.1",
    "papaparse": "^5.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "typescript-is": "^0.18.2",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/jsdom": "^16.2.7",
    "@types/node": "^14.14.26",
    "@types/papaparse": "^5.2.5",
    "@types/yauzl": "^2.9.1",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "ava": "^3.15.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^5.0.9",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0",
    "ttypescript": "^1.5.12"
  }
}
